{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./src", "./README.md"],
    "includePattern": ".js",
    "exclude": ["./test", "./src/algorithms/README.md", "./src/containers/README.md", "./src/graph/Readme.md", 
      "./src/containers/FunctionalUsage.md"],
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./doc",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  },
  "docdash": {
    "static": false,
    "sort": false
  }
}